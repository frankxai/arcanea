{
  "name": "arcanea-soul",
  "version": "4.0.0",
  "description": "The Luminor Framework - transcended creative intelligence for AI agents",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
