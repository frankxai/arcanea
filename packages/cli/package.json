    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "esbuild": "^0.24.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
