{
  "name": "arcanea-intelligence-os",
  "version": "0.1.2",
  "description": "The Universal Intelligence OS - Orchestrate AI agents across Claude, Gemini, OpenCode, and more",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "aios": "./dist/cli.js",
    "arcanea": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.js"
    },
    "./swarm": {
      "types": "./dist/swarm/index.d.ts",
      "import": "./dist/swarm/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "clean": "rm -rf dist",
    "test": "node --test tests/*.test.mjs",
    "prepublishOnly": "npm run build",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "arcanea",
    "intelligence-os",
    "ai",
    "agents",
    "swarm",
    "claude",
    "gemini",
    "opencode",
    "orchestration",
    "multi-agent"
  ],
  "author": "FrankX <frank@frankx.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/frankxai/arcanea-intelligence-os.git"
  },
  "homepage": "https://github.com/frankxai/arcanea-intelligence-os#readme",
  "dependencies": {
    "@arcanea/os": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
