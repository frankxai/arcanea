{
  "name": "@arcanea/skill-registry",
  "version": "0.1.0",
  "description": "Dynamic skill discovery and activation with capability matching, versioning, and Guardian-aware routing",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    }
  },
  "scripts": {
