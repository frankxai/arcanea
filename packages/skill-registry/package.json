  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frankxai/arcanea.git",
    "directory": "packages/skill-registry"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "typescript": "^5.5.0"
  }
}
