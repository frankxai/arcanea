{
  "name": "arcanea-library-premium",
  "version": "3.0.0",
  "description": "Arcanea Magical System - 38 Guardian Intelligence with Multi-Dimensional Analysis",
  "main": "arcanea-magical-agent.js",
  "scripts": {
    "scan": "node arcanea-magical-agent.js scan",
    "search": "node arcanea-magical-agent.js search",
    "stats": "node arcanea-magical-agent.js stats",
    "analyze": "node arcanea-magical-agent.js analyze",
    "recommend": "node arcanea-magical-agent.js recommend",
    "guardians": "node arcanea-magical-agent.js guardians",
    "workflows": "node arcanea-magical-agent.js workflows",
    "run": "node arcanea-magical-agent.js run",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy": "vercel --prod",
    "web": "npm run dev",
    "magical": "npm run dev",
    "install-cli": "npm install -g chalk ora boxen gradient-string",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "framer-motion": "^10.16.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "boxen": "^5.1.2",
    "gradient-string": "^2.0.2",
    "sharp": "^0.32.0",
    "express": "^4.18.0",
    "multer": "^1.4.0",
    "cors": "^2.8.0",
    "react-dropzone": "^14.2.0",
    "react-intersection-observer": "^9.5.0",
    "use-debounce": "^9.0.0",
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.6.0",
    "openai": "^4.24.0"
  },
  "devDependencies": {
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "nodemon": "^3.0.0",
    "concurrently": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "@playwright/test": "^1.41.0",
    "@types/jest": "^29.5.0",
    "babel-jest": "^29.7.0",
    "wait-on": "^7.2.0"
  },
  "keywords": [
    "arcanea",
    "library",
    "gallery",
    "file-management",
    "ai",
    "superintelligence",
    "guardian-system",
    "frankx",
    "visual-assets",
    "intelligent-search",
    "content-analysis",
    "opencode",
    "premium-ui",
    "local-library",
    "ai-categorization"
  ],
  "author": "FrankX",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/frankx/arcanea-library-premium"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "arcanea": {
    "version": "3.0.0",
    "intelligence": "magical",
    "guardians": 38,
    "dimensions": 7,
    "features": [
      "38-guardian-detection",
      "7-dimensional-analysis",
      "semantic-search",
      "ai-recommendations",
      "learning-system",
      "magical-ui",
      "aurora-effects",
      "3d-particles",
      "workflow-automation",
      "smart-notifications",
      "visual-analysis",
      "guardian-personality"
    ],
    "skills": [
      "guardian-detection",
      "visual-analysis",
      "semantic-search",
      "recommendation-engine",
      "workflow-automation",
      "learning-system"
    ],
    "triggers": [
      "file-watcher",
      "smart-notifications",
      "workflow-triggers"
    ]
  }
}