{
  "name": "arcanea-ai",
  "version": "1.0.0",
  "description": "Arcanea.ai - Premium AI-powered spatial worldbuilding experience",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "cross-env ANALYZE=true next build",
    "deploy:prod": "npm run build && npm run start",
    "security:audit": "npm audit --audit-level=moderate",
    "performance:check": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-results.json"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@react-three/fiber": "^8.15.0",
    "@react-three/drei": "^9.88.0",
    "three-stdlib": "^2.28.0",
    "@react-three/xr": "^5.5.0",
    "three": "^0.160.0",
    "@types/three": "^0.160.0",
    "framer-motion": "^10.16.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-toast": "^1.1.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "lucide-react": "^0.292.0",
    "zustand": "^4.4.0",
    "@supabase/supabase-js": "^2.38.0",
    "socket.io-client": "^4.7.0",
    "swr": "^2.2.0",
    "@anthropic-ai/sdk": "^0.24.0",
    "openai": "^4.28.0",
    "@google/generative-ai": "^0.15.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.2.0",
    "tailwindcss": "^3.3.0",
    "postcss": "^8.4.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.51.0",
    "eslint-config-next": "^14.0.0"
  }
}