{
  "name": "arcanea-game-portal",
  "version": "1.0.0",
  "description": "Arcanean Portal - Intelligent Game Launcher & Community Platform",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "backend:dev": "nodemon backend/index.js",
    "backend:prod": "node backend/index.js",
    "ai-dev": "python ai-core/main.py",
    "setup": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "dev:full": "concurrently \"npm run dev\" \"npm run backend:dev\" \"npm run ai-dev\""
  },
  "keywords": [
    "gaming",
    "portal",
    "ai",
    "arcanea",
    "game-launcher",
    "community",
    "recommendations",
    "elemental-alignment"
  ],
  "author": "Arcanea Collective",
  "license": "Arcanean Collective License",
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "express": "^4.18.0",
    "mongoose": "^7.5.0",
    "redis": "^4.6.0",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.0",
    "axios": "^1.5.0",
    "framer-motion": "^10.16.0",
    "@heroicons/react": "^2.0.0",
    "recharts": "^2.8.0",
    "zustand": "^4.4.0",
    "react-query": "^3.39.0",
    "tensorflow": "^4.10.0",
    "brain.js": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/bcryptjs": "^2.4.0",
    "nodemon": "^3.0.0",
    "concurrently": "^8.2.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "eslint": "^8.50.0",
    "eslint-config-next": "^14.0.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arcanea/arcanea-game-portal"
  },
  "bugs": {
    "url": "https://github.com/arcanea/arcanea-game-portal/issues"
  },
  "homepage": "https://portal.arcanea.ai",
  "arcanean": {
    "elemental_alignment": "all",
    "primary_guardian": "@symphony-conductor",
    "compatibility": ["next.js", "vercel", "node.js", "mongodb", "redis"],
    "community_features": true,
    "ai_integration": true,
    "transcendence_level": "superintelligence"
  }
}