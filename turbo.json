{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env.production",
        ".env.local"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**",
        "build/**",
        ".expo/**"
      ],
      "env": [
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "DATABASE_URL",
        "DIRECT_URL",
        "GOOGLE_GENERATIVE_AI_API_KEY"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^build"],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".eslintrc.js",
        ".eslintrc.json",
        ".eslintignore"
      ],
      "outputs": []
    },
    "type-check": {
      "dependsOn": ["^build"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "tsconfig.json"
      ],
      "outputs": [
        "*.tsbuildinfo"
      ]
    },
    "test": {
      "dependsOn": ["^build"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "tests/**/*.test.mjs",
        "tests/**/*.test.ts"
      ],
      "outputs": [],
      "env": [
        "DATABASE_URL",
        "NEXT_PUBLIC_SUPABASE_URL"
      ]
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "db:generate": {
      "cache": false,
      "inputs": [
        "prisma/schema.prisma"
      ],
      "outputs": [
        "generated/**"
      ]
    },
    "db:push": {
      "cache": false
    },
    "db:migrate": {
      "cache": false
    },
    "db:seed": {
      "cache": false,
      "dependsOn": ["db:generate"]
    },
    "db:studio": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "outputs": [],
      "inputs": [
        "**/*.{js,jsx,ts,tsx,json,md,mdx,css,html,yml,yaml}"
      ]
    },
    "format:check": {
      "outputs": []
    }
  },
  "globalDependencies": [
    "**/.env.*local",
    ".env",
    "tsconfig.json",
    "package.json"
  ],
  "globalEnv": [
    "NODE_ENV",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "CI"
  ]
}
