{
  "framework": "nextjs",
  "buildCommand": "cd ../.. && pnpm build --filter=@arcanea/studio",
  "outputDirectory": ".next",
  "installCommand": "cd ../.. && pnpm install",
  "devCommand": "cd ../.. && pnpm dev --filter=@arcanea/studio",
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "app/**/*.tsx": {
      "runtime": "nodejs18.x"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}